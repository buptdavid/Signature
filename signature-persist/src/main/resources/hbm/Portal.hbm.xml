<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	
<hibernate-mapping package="com.juicy.signature.persist.po">
	<class
		name="Portal"
		table="PORTAL"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"></cache>
		<id
			name="Id"
			type="integer"
			column="id"
		>
		</id>
		
		<property
			name="MaxDefaultCount"
			column="max_default_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="MaxPersonalizeCount"
			column="max_personalize_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="MaxNum"
			column="max_num"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="ExperienceDay"
			column="experience_day"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="MaxCountOriginal"
			column="max_count_original"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="ManagerId"
			column="manager_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<property
			name="MdfTime"
			column="mdf_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		
		<property
			name="GmtTime"
			column="gmt_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>

	</class>	
</hibernate-mapping>