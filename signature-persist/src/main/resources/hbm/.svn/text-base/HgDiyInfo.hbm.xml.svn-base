<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.juicy.signature.persist.po">
	<class
		name="HgDiyInfo"
		table="HG_DIY_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"></cache>
		<id
			name="Id"
			type="integer"
			column="DIY_INFO_ID"
		>
		</id>

		<property
			name="CustomerId"
			column="CUSTOMER_ID"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="MessageTypeId"
			column="MESSAGE_TYPE_ID"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DiyInfoName"
			column="DIY_INFO_NAME"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="DiyInfoCurType"
			column="DIY_INFO_CUR_TYPE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="DiyInfoCreatetime"
			column="DIY_INFO_CREATETIME"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="DiyInfoLangType"
			column="DIY_INFO_LANG_TYPE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DiyInfoContent"
			column="DIY_INFO_CONTENT"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="DiyInfoPages"
			column="DIY_INFO_PAGES"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="DiyInfoCanedit"
			column="DIY_INFO_CANEDIT"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Foldername"
			column="FOLDERNAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Folderzip"
			column="FOLDERZIP"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="IsDefault"
			column="IS_DEFAULT"
			type="java.lang.Byte"
			not-null="false"
			length="3"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="java.lang.Byte"
			not-null="true"
			length="3"
		/>
		<property
			name="LibraryClassId"
			column="LIBRARY_CLASS_ID"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LibraryClassName"
			column="LIBRARY_CLASS_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<many-to-one
			name="Original"
			column="ORIGINAL_ID"
			class="SignatureOriginal"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Library"
			column="LIBRARY_ID"
			class="SignatureLibrary"
			not-null="false"
			lazy="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>