<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.juicy.signature.persist.po">
	<class
		name="StUsers"
		table="st_users"
	>
		<meta attribute="sync-DAO">false</meta>
		
		<!-- ä¸»é”®ID -->
		<id
			name="Id"
			type="integer"
			column="user_id"
		>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="LoginName"
			column="login_name"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="Password"
			column="password"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="Enabled"
			column="enabled"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="IfIniUser"
			column="IF_INI_USER"
			type="boolean"
			not-null="true"
			length="1"
		/>
		<property
			name="UserPeriodOfValidity"
			column="USER_PERIOD_OF_VALIDITY"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="PwdModiTime"
			column="pwd_modi_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="UserPasswordquestion1"
			column="USER_PASSWORDQUESTION1"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserPasswordkey1"
			column="USER_PASSWORDKEY1"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserPasswordquestion2"
			column="USER_PASSWORDQUESTION2"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserPasswordkey2"
			column="USER_PASSWORDKEY2"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserPasswordquestion3"
			column="USER_PASSWORDQUESTION3"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserPasswordkey3"
			column="USER_PASSWORDKEY3"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UserType"
			column="USER_TYPE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="64"
		/>
		<!-- please tell Joe Hudson that the type 'longsysname' could not be resolved.. defaulting to java.lang.String -->
		<property
			name="Phone"
			column="phone"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="DeptId"
			column="dept_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RegionCode"
			column="region_code"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Creator"
			column="creator"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="CreateTime"
			column="create_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Manager"
			column="manager"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Optlock"
			column="optlock"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LastLoginTime"
			column="last_login_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="LoginCount"
			column="login_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="VerifyCode"
			column="verify_code"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="VerifyInvalideTime"
			column="verify_invalide_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="500"
		/>

	</class>	
</hibernate-mapping>