package com.juicy.signature.action.base;

import com.juicy.signature.service.SuggestService;
import com.juicy.signature.service.SystemManage;
import com.opensymphony.xwork2.ActionSupport;

/**
 * 系统管理基本Action
 *
 * @author 路卫杰
 * @version <p>Nov 25, 2011 创建</p>
 */
public class SystemManageBaseAction extends ActionSupport {

	/** serialVersionUID */
	private static final long serialVersionUID = 1L;
	
	protected SystemManage systemManage;
	
	protected SuggestService suggestService;
	
	/** 操作后的信息 */
	private String message;


	public String getMessage() {
		return message;
	}


	public void setMessage(String message) {
		this.message = message;
	}


	public SystemManage getSystemManage() {
		return systemManage;
	}


	public void setSystemManage(SystemManage systemManage) {
		this.systemManage = systemManage;
	}


	public SuggestService getSuggestService() {
		return suggestService;
	}


	public void setSuggestService(SuggestService suggestService) {
		this.suggestService = suggestService;
	}
	
	
	

}
