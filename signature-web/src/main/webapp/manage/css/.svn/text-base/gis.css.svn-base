.gisbox{ width:98%; margin:0 auto;}
a {text-decoration: none;} /* 链接无下划线,有为underline */ 
a:link {color: #000000;text-decoration: none;} /* 未访问的链接 */
a:visited {color: #000000;text-decoration: none;} /* 已访问的链接 */
a:hover {color: #FF3300;text-decoration: none;} /* 鼠标在链接上 */ 
a:active {color: #000000;text-decoration: none;} /* 点击激活链接 */
/*放大*/
a.fangda:link{ width:60px; height:51px; line-height:51px; padding-left:35px; background:url(../images/gistype1_11.gif) no-repeat; font-size:14px; font-weight:bolder; display:block;}
a.fangda:visited{width:60px; height:51px; line-height:51px; padding-left:35px; background:url(../images/gistype1_11.gif) no-repeat; font-size:14px; font-weight:bolder; display:block;}
a.fangda:hover{width:60px; height:51px; line-height:51px; padding-left:35px; background:url(../images/gisover_11.gif) no-repeat; font-size:14px; font-weight:bolder; display:block;}
a.fangda:active{width:60px; height:51px; line-height:51px; padding-left:35px; background:url(../images/gisactive_11.gif) no-repeat; font-size:14px; font-weight:bolder; display:block;}
