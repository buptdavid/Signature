/* 导航链接未选中 */
a.auselect {
	color: #333333;
	display:block;
	height:38px;
	line-height:38px;
}
a.auselect:link {
	color: #353535;
}
a.auselect:visited {
	color: #fff;
}
a.auselect:hover {
	color: #fff;
	background:url(../images/marrow_09yellow.gif) bottom center no-repeat;
}
a.auselect:active {
	color:red;
	background:url(../images/marrow_09red.gif) bottom center no-repeat;
}
/*选中状态*/
a.aselect {
	display:block;
	color: #ffffff;
	font-size:14px;
	font-weight:bolder;
	height:38px;
	padding-top:0px;
	line-height:38px;
}
a.aselect:link {
	color: #ffffff;
	background:url(../images/marrow_09.gif) bottom center no-repeat;
}
a.aselect:visited {
	color:red;
	background:url(../images/marrow_09.gif) bottom center no-repeat;
}
a.aselect:hover {
	color: red;
	background:url(../images/marrow_09red.gif) bottom center no-repeat;
}
a.aselect:active {
	color: red;
	background:url(../images/marrow_09red.gif) bottom center no-repeat;
}
/*链接效果与颜色定义*/
a {
	text-decoration: none;
} /* 链接无下划线,有为underline */
a:link {
	color: #000000;
	text-decoration: none;
} /* 未访问的链接 */
a:visited {
	color: #000000;
	text-decoration: none;
} /* 已访问的链接 */
a:hover {
	color: #FF0000;
	text-decoration: underline;
} /* 鼠标在链接上 */
a:active {
	color: #000000;
	text-decoration: none;
} /* 点击激活链接 */


/*分页文字链接效果与颜色定义*/
.page_link a {
	color: #0759B4;
	text-decoration: none;
} /* 链接无下划线,有为underline */
.page_link a:link {
	color: #0759B4;
	text-decoration: underline;
} /* 未访问的链接 */
.page_link a:visited {
	color: #0759B4;
	text-decoration: none;
} /* 已访问的链接 */
.page_link a:hover {
	color: #0759B4;
	font-weight:bold;
	text-decoration: underline;
} /* 鼠标在链接上 */
.page_link a:active {
	color: #000000;
	text-decoration: none;
} /* 点击激活链接 */

/* 栏目标题文字链接的CSS定义 */
a.Class:link {
	color: #ffffff;
}
a.Class:visited {
	color: #ffffff;
}
a.Class:hover {
	color: red;
}
a.Class:active {
