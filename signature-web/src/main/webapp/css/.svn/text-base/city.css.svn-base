.sub01 .tese {
	BORDER-BOTTOM: #dfdada 1px solid; BORDER-LEFT: #dfdada 1px solid; WIDTH: 919px; BACKGROUND: #f5f7fb; HEIGHT: 140px; MARGIN-LEFT: 20px; BORDER-TOP: #dfdada 1px solid; BORDER-RIGHT: #dfdada 1px solid
