.page_number_b {
	width: 100%;
	height: 20px;
	text-align: right;
	float: right;
	margin: 10px 0 10px 0;
	padding-right: 0;
}

.page_number_b1 {
	width: 100%;
	height: 20px;
	text-align: right;
	float: right;
	margin: 50px 0 10px 0;
	padding-right: 0;
}

.page_number_a {
	width: 98%;
	float: right;
}

.page_number_a li {
	display: inline;
	white-space: nowrap;
	width: auto;
	line-height: 18px;
	padding: 0 2px;
	float: right;
	color: #00527f;
}

.page_number_a a:active,.page_number_a a:visited,.page_number_a a:link {
	display: block;
	text-decoration: none;
	width: auto;
	line-height: 18px;
	padding: 0 3px;
	color: #00527f;
	background-color: #f2f8ff;
	border: 1px solid #b2d4ff;
}

.page_number_a a:hover {
	color: red;
	background-color: #ffd482;
}

.page_number_a a:focus {
	font-size: 14pt;
	text-decoration: underline;
	color: blue;
}

.page_number_c {
	color: red;
}

.page_number_c a:link,.page_number_c a:active,.page_number_c a:visited {
	color: red;
	background-color: #ffd482;
}

.red_a {
	color: #C00;
}

