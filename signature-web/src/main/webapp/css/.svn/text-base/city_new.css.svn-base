BODY {
	LINE-HEIGHT: 18px; BACKGROUND-COLOR: #fff; FONT-SIZE: 12px
}
.m-slide {
	POSITION: relative; BACKGROUND-COLOR: #2e2925; WIDTH: 710px; HEIGHT: 200px; OVERFLOW: hidden
}
.m-slide .content A {
	POSITION: absolute; TOP: 0px; LEFT: 0px
}
.m-slide .content A H4 {
	DISPLAY: none
}
.m-slide .content A {
	VISIBILITY: hidden
}
.m-slide .control {
	Z-INDEX: 1000; POSITION: absolute; MARGIN: 0px; BOTTOM: 6px; RIGHT: 5px
}
.m-slide .control LI {
	LINE-HEIGHT: 22px; MARGIN: 0px 2px 0px 0px; WIDTH: 22px; DISPLAY: inline; BACKGROUND: url(../images/icon.png) 0px 0px; FLOAT: left; HEIGHT: 22px; CURSOR: pointer
}
.m-slide .control LI.current {
	WIDTH: 22px; BACKGROUND: url(../images/icon.png) 0px -30px
}
.m-slide .control LI SPAN {
	MARGIN: 0px 0px 0px 8px; COLOR: #fff; FONT-WEIGHT: bold
}
.m-slide .control LI.current SPAN {
	COLOR: #000; MARGIN-LEFT: 9px
}
